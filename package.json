    "description": "Modern VK API SDK for Node.js",
    "main": "index.js",
    "scripts": {
        "lint": "eslint ./src",
        "lint:fix": "npm run lint -- --fix",
        "test": "mocha lib/tests",
        "build": "npm run-script babel",
        "babel": "babel src/ --out-dir lib/",
        "watch": "npm run-script babel -- --watch",
        "prepare": "npm run-script babel && npm run-script test"
    },
    "engines": {
        "node": ">=8"
    },
    "files": [
        "lib"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/negezor/vk-io.git"
    },
    "keywords": [
        "vk",
        "api",
        "sdk",
        "bot",
        "node",
        "js",
        "es6",
        "es7"
    ],
    "author": "Vladlen <negezor@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/negezor/vk-io/issues"
    },
    "homepage": "https://github.com/negezor/vk-io#readme",
    "devDependencies": {
        "@babel/cli": "^7.0.0-beta.40",
        "@babel/core": "^7.0.0-beta.40",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
        "@babel/preset-env": "^7.0.0-beta.40",
        "chai": "^4.1.2",
        "eslint": "^4.18.0",
        "eslint-config-airbnb-base": "^12.1.0",
        "eslint-plugin-import": "^2.8.0",
        "mocha": "^5.0.1"
    },
